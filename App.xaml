<Application x:Class="TaskManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TaskManager"
             xmlns:converters="clr-namespace:TaskManager.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converters:BoolToStrikethroughConverter x:Key="BoolToStrikethroughConverter"/>
            <converters:BoolToForegroundConverter x:Key="BoolToForegroundConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
